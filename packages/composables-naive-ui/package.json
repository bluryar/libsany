{
  "name": "@bluryar/composables-naive-ui",
  "version": "0.0.8",
  "private": false,
  "description": "⚡ Vue3组合式API - 与 naive-ui 强相关",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/libsany#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/libsany.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/libsany/issues"
  },
  "keywords": [
    "vue3",
    "composables",
    "hooks"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production unbuild",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
    "bump:minor": "npx bumpp minor --no-commit --no-tag --no-push --yes",
    "bump:patch": "npx bumpp patch --no-commit --no-tag --no-push --yes",
    "release": "npm publish",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "naive-ui": "^2.34.3",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@bluryar/composables": "workspace:*",
    "@bluryar/shared": "workspace:*",
    "@vueuse/core": "^9.11.1",
    "@vueuse/shared": "^9.11.1",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.3",
    "type-fest": "^3.5.4",
    "vue-demi": "latest",
    "vue-request": "2.0.0-rc.4"
  },
  "devDependencies": {
    "vue": "^3.2.45",
    "vue-demi": "latest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
