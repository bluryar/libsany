    "./dist/presetNaiveThemes": {
      "types": "./dist/presetNaiveThemes.d.ts",
      "import": "./dist/presetNaiveThemes.mjs"
    },
    "./dist/naiveMultiTheme": {
      "types": "./dist/naiveMultiTheme.d.ts",
      "import": "./dist/naiveMultiTheme.mjs"
    },
