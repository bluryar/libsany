      "types": "./dist/fileReader.d.ts",
      "import": "./dist/fileReader.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
