{
  "name": "@bluryar/composables",
  "version": "0.1.0",
  "private": false,
  "description": "⚡ Vue3组合式API",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/libsany#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/libsany.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/libsany/issues"
  },
  "keywords": [
    "vue3",
    "composables",
    "hooks"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production unbuild",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
    "bump:minor": "npx bumpp minor --no-commit --no-tag --no-push --yes",
    "bump:patch": "npx bumpp patch --no-commit --no-tag --no-push --yes",
    "release": "npm publish",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "@vueuse/core": ">=10.0.0",
    "lodash-es": "*",
    "vue": ">=3.0.0",
    "vue-request": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "lodash-es": {
      "optional": true
    },
    "vue-request": {
      "optional": true
    },
    "@vueuse/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@bluryar/shared": "workspace:latest",
    "type-fest": "^3.11.1"
  },
  "devDependencies": {
    "@vueuse/core": "^10.1.2",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4",
    "vue-request": "2.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
