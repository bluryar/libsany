{
  "name": "@bluryar/libsany",
  "type": "module",
  "version": "0.1.27",
  "private": true,
  "description": "⚙️ Vue3开发者自用的常用工具库",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/libsany#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/libsany.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/libsany/issues"
  },
  "keywords": [
    "libs",
    "vue3",
    "ts",
    "utils"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:vue-ts\"",
    "dev:vue-ts": "nr -F ./playgrounds/vue-ts dev",
    "build": "nr -r build",
    "build:pkg": "nr -F ./packages/** build",
    "build:plg": "nr -F ./playgrounds/** build",
    "stub": "nr -r stub",
    "test": "vitest",
    "check:types": "tsc --noEmit",
    "clear:modules": "rimraf --glob ./{packages, playgrounds}/*/node_modules && rimraf ./node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "concurrently \"npm:preview:vue-ts\"",
    "preview:vue-ts": "nr -F ./playgrounds/vue-ts preview",
    "bump:patch": "nr -r bump:patch && npx bumpp patch --all --yes",
    "bump:minor": "nr -r bump:minor && npx bumpp minor --all --yes",
    "bump:major": "nr -r bump:major && npx bumpp major --all --yes",
    "release": "nr -r release",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@bluryar/commitlint-config": "^1.1.2",
    "@bluryar/eslint-config-vue": "^1.1.2",
    "@bluryar/shared": "workspace:*",
    "@commitlint/config-conventional": "^17.6.6",
    "@rushstack/eslint-patch": "^1.3.2",
    "@turf/turf": "^6.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.18",
    "@types/node-fetch": "^2.6.4",
    "@types/tinycolor2": "^1.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.2.1",
    "@vueuse/shared": "^10.2.1",
    "bumpp": "^9.1.1",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.6",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.6.1",
    "dotenv": "^16.3.1",
    "esbuild": "0.18.10",
    "eslint": "^8.43.0",
    "eval": "^0.1.8",
    "fake-indexeddb": "^4.0.1",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mlly": "^1.4.0",
    "msw": "^1.2.2",
    "naive-ui": "^2.34.4",
    "node-fetch": "^3.3.1",
    "pnpm": "^7.33.2",
    "taze": "^0.10.3",
    "tinycolor2": "^1.6.0",
    "tsx": "^3.12.7",
    "type-fest": "^3.12.0",
    "typescript": "~5.0.4",
    "unbuild": "^1.2.1",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.30",
    "vitest": "^0.31.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.3",
    "vue-types": "^5.1.0"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,cjs,mjs,cts,mts,yaml,md,yml}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
