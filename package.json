{
  "name": "@bluryar/libsany",
  "type": "module",
  "version": "0.1.5",
  "private": true,
  "description": "⚙️ Vue3开发者自用的常用工具库",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/libsany#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/libsany.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/libsany/issues"
  },
  "keywords": [
    "libs",
    "vue3",
    "ts",
    "utils"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:vue-ts\"",
    "dev:vue-ts": "nr -F ./playgrounds/vue-ts dev",
    "build": "nr -r build",
    "stub": "nr -r stub",
    "test": "vitest",
    "check:types": "tsc --noEmit",
    "clear:modules": "rimraf --glob ./{packages, playgrounds}/*/node_modules && rimraf ./node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bump:patch": "nr -r bump:patch && npx bumpp patch --all --yes",
    "bump:minor": "nr -r bump:minor && npx bumpp minor --all --yes",
    "bump:major": "nr -r bump:major && npx bumpp major --all --yes",
    "release": "nr -r release",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@bluryar/commitlint-config": "^1.1.0",
    "@bluryar/eslint-config-vue": "^1.1.0",
    "@bluryar/shared": "workspace:*",
    "@commitlint/config-conventional": "^17.6.5",
    "@rushstack/eslint-patch": "^1.3.0",
    "@turf/turf": "^6.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.1.2",
    "@vueuse/shared": "^10.1.2",
    "bumpp": "^9.1.1",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.5",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.6.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pnpm": "^7.33.0",
    "taze": "^0.10.2",
    "tsx": "^3.12.7",
    "type-fest": "^3.11.1",
    "typescript": "5.1.3",
    "unbuild": "^1.2.1",
    "unplugin-auto-import": "^0.16.4",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5",
    "vue-types": "^5.0.3"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,cjs,mjs,cts,mts,yaml,md,yml}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
